package com.myspace.ruleselection;

import java.lang.Number;

rule "Initialize"
	dialect "mvel"
	ruleflow-group "initialize"
	when
		not (Transaction( this != null )) 
	then
		Transaction fact0 = new Transaction();
		fact0.setType( "MUTUAL-FUNDS-COST-BASIS-SETTINGS" );
		fact0.setProductCode( "10" );
		insertLogical( fact0 );
end
